#include<stdio.h>
#include<conio.h>
int main()
{
int  i,j,row,col,a[10][10];
clrscr();

printf("enter row and column of the array\n");
scanf("%d %d",&row,&col);

printf("enter the array\n");
for(i=0;i<row;i++)
{
for(j=0;j<col;j++)
{
scanf("%d",&a[i][j]);
}
}

printf("transpose of the array is \n");
 for(i=0;i<row;i++)
{
for(j=0;j<col;j++)
{
printf("%d\t",a[j][i]);
}
printf("\n");
}


getch();
return 0;
}
